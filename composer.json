{
    "name": "oleksiivelychko/laravel-dashboard",
    "homepage": "https://github.com/oleksiivelychko/laravel-dashboard",
    "type": "project",
    "description": "Dashboard management application based on PHP8/Laravel10. No JetStream. No Liveware. No Tailwind. Completely dockerized and fits great for local development.",
    "keywords": ["laravel", "php8", "docker", "docker-compose", "dashboard", "starter-kit"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksii Velychko",
            "email": "oleksiivelychko@icloud.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-pdo": "*",
        "darkaonline/l5-swagger": "^8.5.1",
        "guzzlehttp/guzzle": "^7.7.0",
        "laravel/framework": "^10.15.0",
        "pusher/pusher-php-server": "^7.2.3",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^13.3.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8.1",
        "barryvdh/laravel-ide-helper": "^2.13.0",
        "fakerphp/faker": "^1.23.0",
        "laravel/tinker": "^2.8.1",
        "mockery/mockery": "^1.6.4",
        "nunomaduro/collision": "^7.7.0",
        "phpunit/phpunit": "^10.2.6",
        "spatie/laravel-ignition": "^2.2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-install-cmd": [
            "@php artisan migrate:refresh --force",
            "@php artisan db:seed --force"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
